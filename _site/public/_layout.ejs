<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<%- partial("_themes/default/_head") %>
    <!-- Google Analytics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', '<%- analytics %>', 'mshanken.com');
        ga('send', {"hitType":"pageview","page":"<%- current.source %>", "section":"<%- current.path[0] %>"});
    </script>
    <!-- End Google Analytics -->
</head>
	
<body>
	
	<div id="wrapperBox" class="addclear">
	&nbsp;
		<div id="heading">

			<table id="header" width="736" border="0" cellspacing="0" cellpadding="0">
			<%- partial("_themes/default/_header") %>
			<%- partial("_themes/default/_navbar") %>
			<% if ( current.path[0] !== "index"  && current.source !== "contact"  && current.source !== "privatepolicy"  && current.source !== "404" ) { %>
				<%- partial("_themes/default/_menu") %>
				<%- partial("_themes/default/_subnavbar") %>
			<% } %>
			</table>

		</div>

		<div id="contentWrapper">
			<% if ( current.path[0] === "index"  || current.source === "contact"  || current.source === "privatepolicy"  || current.source === "404" ) { %>
	      		<div class="oneColumnContent">
	      			<%- yield %>
	      		</div>
      		<% } else  { %>
      			<%- partial(current.path[0] + "/_contentbody", { yield: yield }) %>
      		<% } %>
      	</div>

      	<%- partial("_themes/default/_footer") %>
    &nbsp;
	</div>
	<script language="javascript" type="text/javascript" src="/js/rollovers.js"></script>
	<script language="javascript" type="text/javascript" src="/js/ws-adplace.js"></script>
</body>
</html>
    	
