<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<%- partial("_themes/v2/_head") %>
</head>
	
<body>
	
	<nav id="navbar" class="container bg-primary" role="menu">
		<div class="row">
			<div class="col-12">
				<%- partial("_themes/v2/_navbar") %>
			</div>
		</div>
	</header>
	
	
	<% if ( current.path[0] === "index"  || current.source === "404" ) { %>
	    <%- yield %>
	<% } else  { %>
		<%#- yield %>
      	<%- partial("_themes/v2/partials/_content", { yield: yield }) %>
    <% } %>
	
	<footer id="footer" class="container footer" role="footer">
		<div class="row">
			<div class="col-12">
				<%- partial("_themes/v2/_footer") %>
			</div>
		</div>
	</div>
    
    
</body>
</html>
    	
