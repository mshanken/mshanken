<!doctype html>
<html class="no-js" lang="">
    <head>
        <%- partial("_themes/bootstrap/_heading") %>
    </head>
    <body>
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->

        <header class="header">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 text-center">
                        <%- partial("_themes/bootstrap/_header") %>
                    </div>
                </div>
                 <div class="row">
                    <div class="col-xs-12">
                        <%- partial("_themes/bootstrap/_navbar") %>
                    </div>
                </div>
                 <div class="row">
                    <div class="col-xs-12">
                        <% if ( current.path[0] !== "index"  && current.source !== "contact"  && current.source !== "privatepolicy"  && current.source !== "404" ) { %>
                            <%- partial("_themes/bootstrap/_menu") %>
                            <%- partial("_themes/bootstrap/_subnavbar") %>
                        <% } %>
                    </div>
                </div>
            </div>
        </header>

        <div class="container main">
            <div class="row">
                <% if ( current.path[0] === "index"  || current.source === "contact"  || current.source === "privatepolicy" || current.source === "404" ) { %>
                        <%- yield %>
                <% } else  { %>
                    <%- partial(current.path[0] + "/_contentbody", { yield: yield }) %>
                <% } %>
            </div>
        </div>

        <footer class="footer">
            <div class="container">
                <%- partial("_themes/bootstrap/_footer") %>
            </div>
        </footer>

        <%- partial("_themes/bootstrap/_scripts") %>
    </body>
</html>
