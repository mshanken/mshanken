<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<%- partial("_includes/_head") %>
</head>
	
<body>
	
	<div id="wrapperBox" class="addclear">
		<div id="heading">

			<table id="header" width="736" border="0" cellspacing="0" cellpadding="0">
			<%- partial("_includes/_header") %>
			<%- partial("_includes/_navbar") %>
			<% if ( current.path[0] !== "index"  && current.source !== "contact"  && current.source !== "privatepolicy" ) { %>
				<%- partial("_includes/_menu") %>
				<%- partial("_includes/_subnavbar") %>
			<% } %>
			</table>
			
		</div>

		<div id="contentWrapper">
			<% if ( current.path[0] === "index"  || current.source === "contact"  || current.source === "privatepolicy" ) { %>
      			<%- yield %>
      		<% } else  { %>
      			<%- partial(current.path[0] + "/_contentbody", { yield: yield }) %>
      		<% } %>
      	</div>

      	<%- partial("_includes/_footer") %>
      	
	</div>

</body>
</html>
    	
