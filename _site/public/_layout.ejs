<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
	<head>
		<%- partial("_themes/default/partials/_headingtags") %>
		<%- partial("_themes/default/partials/_styles") %>
	    <% if (ga) { %>
	    	<%- partial("_themes/default/partials/_ga") %>
	    <% 	} %>
	
		<!--[if lt IE 9]>
	      <script src="/js/vendor/html5shiv.js"></script>
	      <script src="/js/vendor/respond.src.js"></script>
	    <![endif]-->
		<script src="/js/vendor/modernizr.js"></script>
		
	</head>
	
<body>
	
	<div class="loading">Loading&#8230;</div>
	<%- partial("_themes/default/_navbar") %><!-- #header -->
	
	<% if ( current.path[0] === "index"  || current.path[0] === "events"  || current.source === "404" ) { %>
	    <%- yield %>
	<% } else if (current.source === "privacypolicy") { %>
		<main class="container g-py-40 g-mt-60 g-mt-140--md">
		<div class="row justify-content-center" data-animation="fadeIn" data-animation-delay="0" data-animation-duration="1000">
			<article id="article" class="article col-md-8 g-pb-40 g-pb-0--md text-left g-font-size-20" role="main-article">
				<div class="g-ml-40--md">
					<%- yield %>
				</div>
			</article>       
		</div>
	</main>
	<% } else  { %>
		<%- partial("_themes/default/partials/_content", { yield: yield }) %>
    <% } %>
	
	<footer id="footer" role="footer" class="g-brd-top g-brd-primary">
		<%- partial("_themes/default/_footer") %>
	</footer>
    
    <%- partial("_themes/default/partials/_scripts") %>
    
</body>
</html>
    	
