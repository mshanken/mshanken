
<% var cur = public._data[current.path[0]] %>
<% var curClass %>

<% var tmp = current.source %>
<% var type = tmp.split(/[-]+/) %>

<tr>
	<td id="menu" valign="bottom" background="/images/<%=cur.imagebg %>" height="164">
		<% switch (current.path[0]) { 
			case "winespectator": %>
				<p class="wsmsm">
				<% break
			case "cigaraficionado": %>
				<p class="camsm">
				<% break
			case "whiskyadvocate": %>
				<p class="wamsm">
				<% break
			case "marketwatch": %>
				<p class="mwmsm">
				<% break
			case "impact": %>
				<p class="immsm">
				<% break
			case "shankennewsdaily": %>
				<p class="sndmsm">
				<% break
			default: %>
				<p>
				<% break
		} %>
		<% for(var menu in cur.menu){ %>
			<% if( menu === type[0] || cur.menu[menu].url === current.path[1] ){ 
			  	curClass = "current"
			} else {
			  	curClass = ""
			} %>
			<a href="<%= cur.menu[menu].url %>.html" class="<%= curClass %>" target="<%= cur.menu[menu].target %>"><%= cur.menu[menu].title %></a>
		<% }; %>
		</p>
	</td>
</tr>
