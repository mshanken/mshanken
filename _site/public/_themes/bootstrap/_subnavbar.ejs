
<% var tmp = current.source %>
<% var type = tmp.split(/[-]+/) %>
<% var cur = menu %>
<% var elClass %>
<% var curClass %>


<% switch (current.path[0]) { 
	case "winespectator": %>
		<ul class="nav navbar-nav wsmsm">
		<% break
	case "cigaraficionado": %>
		<ul class="nav navbar-nav camsm">
		<% break
	case "whiskyadvocate": %>
		<ul class="nav navbar-nav wamsm">
		<% break
	case "marketwatch": %>
		<ul class="nav navbar-nav mwmsm">
		<% break
	case "impact": %>
		<ul class="nav navbar-nav immsm">
		<% break
	case "shankennewsdaily": %>
		<ul class="nav navbar-nav sndmsm">
		<% break
	default: %>
		<ul>
		<% break
} %>

<% for(var submenu in cur ){ %>
	<% if( cur[submenu].url == current.source ){ 
  		curClass = "current"
  	} else {
  		curClass = ""
  	} %>
	<li><a href="<%= cur[submenu].url %><% if( cur[submenu].title !== "custom media" ){%>.html<%}%>" class="<%= curClass %>" target="<%= cur[submenu].target %>"><%= cur[submenu].title %></a></li>
<% }; %>
	</ul>
	