
<% var cur = public._data[current.path[0]] %>
<% var curData = {} %>
<% var i = 0 %>
<% var tmp = current.source %>
<% var type = tmp.split(/[-]+/) %>

<% for(var menu in cur){ %>
	<% curData[i] = cur[menu] %>
	<% i++ %>
<% }; %>

	
<% switch (current.path[0]) { 
	case "winespectator": %>
		<ul class="nav navbar-nav wsmsm">
		<% break
	case "cigaraficionado": %>
		<ul class="nav navbar-nav camsm">
		<% break
	case "whiskyadvocate": %>
		<ul class="nav navbar-nav wamsm">
		<% break
	case "marketwatch": %>
		<ul class="nav navbar-nav mwmsm">
		<% break
	case "impact": %>
		<ul class="nav navbar-nav immsm">
		<% break
	case "shankennewsdaily": %>
		<ul class="nav navbar-nav sndmsm">
		<% break
	default: %>
		<ul>
		<% break
} %>

<% for(var menu in curData[4]){ %>
	<% if( menu === type[0] || curData[4][menu].url === current.path[1] ){ 
	  	curClass = "current"
	} else {
	  	curClass = ""
	} %>
	<li><a href="<%= curData[4][menu].url %>.html" class="<%= curClass %>" target="<%= curData[4][menu].target %>"><%= curData[4][menu].title %></a></li>
<% }; %>
		</ul>
	